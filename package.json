{
  "name": "mawe.web",
  "description": "Editor for writers.",
  "author": "Markus Koskimies <mkoskim@gmail.com>",
  "version": "0.0.1",
  "main": "./public/electron.js",
  "homepage": "./",
  "private": true,
  "scripts": {
    "dev": "concurrently npm:dev:react npm:dev:electron",
    "dev:electron": "wait-on http://localhost:3000 && electron .",
    "dev:react": "cross-env BROWSER=none npm run react-start",
    "audit": "npm audit --production",
    "build": "npm run react-build && npm run electron-build",
    "electron-build": "electron-builder",
    "react-start": "react-scripts start",
    "react-build": "react-scripts build",
    "react-test": "react-scripts test",
    "react-eject": "react-scripts eject",
    "fresh": "rm -rf build node_modules package-lock.json && npm install"
  },
  "dependencies": {
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@mui/icons-material": "^5.8.4",
    "@mui/material": "^5.8.4",
    "@reduxjs/toolkit": "^1.8.2",
    "electron-better-ipc": "^2.0.1",
    "electron-debug": "^3.2.0",
    "electron-is-dev": "^2.0.0",
    "electron-window-state": "^5.0.3",
    "elementtree": "^0.1.7",
    "fs-extra": "^10.1.0",
    "is-gzip": "^2.0.0",
    "is-hotkey": "^0.2.0",
    "notistack": "^2.0.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-infinite-scroll-component": "^6.1.0",
    "react-redux": "^8.0.2",
    "redux": "^4.2.0",
    "slate": "^0.81.0",
    "slate-history": "^0.66.0",
    "slate-react": "^0.81.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "concurrently": "^7.2.1",
    "cross-env": "^7.0.3",
    "electron": "^19.0.4",
    "electron-builder": "^23.0.3",
    "esm": "^3.2.25",
    "react-scripts": "^4.0.3",
    "wait-on": "^6.0.1"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
